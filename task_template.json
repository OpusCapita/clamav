{
  "default":{
    "name":"${serviceName}",
    "log-driver":"gelf",
    "log-opt":["gelf-address=udp://${logstash_ip}:12201", "tag=\"${serviceName}\""],
    "constraint":["engine.labels.nodetype==worker"],
    "publish":["mode=host,target=8080,published=3036,protocol=tcp"],
    "host":["consul:172.17.0.1"],
    "env":[
           "SERVICE_NAME=${serviceName}",
           "SERVICE_3036_CHECK_HTTP=/",
           "SERVICE_3036_CHECK_INTERVAL=15s",
           "SERVICE_3036_CHECK_TIMEOUT=3s",
           "NODE_ENV=production"
           ],
    "oc-consul-injection":{
    }
  }
}
